void main() {
	int t, n, x,i,j,sum,k,pos,velocity;
	cin>>t>>n;
	for(k=0;k<t;k++) {
		map<int,int>m;
		vector<pair<int,int> > v;
		for(i=0;i<n;i++) {
			cin>>pos>>velocity;
			v.push_back(make_pair(velocity, pos));
		}
		sort(v.begin(), v.end());
		int cnt = 0;
		for(i=1;i<n;i++) {
			for(j=0;j<i;j++) {
				if (v[j].second > v[i].second) {
					cnt++;
				}
			}
		}
		cout<<cnt<<endl;
	}
}